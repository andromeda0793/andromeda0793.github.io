	window.XHR_VS_URL = "https://jobs.dou.ua/vacancies/xhr-load/?category=QA&amp;city=%D0%A5%D0%B0%D1%80%D1%8C%D0%BA%D0%BE%D0%B2";
	var vm = new ItemListManager($("#vacancyListId"), "ul.lt", "li.l-vacancy", window.XHR_VS_URL);

	$(function() {
		$.post('/ajax-unread/', {
			'csrfmiddlewaretoken': window.CSRF_TOKEN
		}).done(function(response) {
			var redis_count = 0;
			var count = parseInt(response);

			if (!count) {
				$('.menu-unread').hide();
				return;
			}

			if (count > 400) {
				count = '<i></i>';
			}

			if(count != redis_count) {
				$('.menu-unread').html(count);
			}
		})
	});

			if (impressions.length) {
				$.post('/ajax-impressions-track/', {
					'csrfmiddlewaretoken': window.CSRF_TOKEN,
					'impressions': JSON.stringify(impressions)
				});
			}
	</script>

